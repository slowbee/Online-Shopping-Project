CREATE TABLE BOOK (    
Sr_no INT ,    
Product_code VARCHAR(20) PRIMARY KEY,   
Product_name VARCHAR(255) NOT NULL,    
Publication VARCHAR(100) NOT NULL,   
Product_rating NUMERIC,    
MRP_Per_product NUMERIC(10, 2) NOT NULL,   
Availability_of_product VARCHAR(15),    
In_stock INT);
